{
  "name": "poppler-ts",
  "version": "0.3.0",
  "description": "Native Node.js binding for Poppler PDF library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/cpp/",
    "binding.gyp",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:native && npm run build:ts",
    "build:native": "node-gyp rebuild",
    "build:ts": "tsc",
    "clean": "node-gyp clean && rm -rf dist",
    "postinstall": "node-gyp rebuild",
    "prepublishOnly": "npm run build",
    "test": "npx ts-node src/test.ts",
    "example": "npx ts-node src/example.ts",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "author": "Maifee Ul Asad<maifeeulasad@gmail.com>",
  "license": "MIT",
  "keywords": [
    "poppler",
    "pdf",
    "native",
    "binding",
    "typescript",
    "pdf-parser",
    "pdf-extraction",
    "pdf-rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maifeeulasad/poppler-ts.git"
  },
  "bugs": {
    "url": "https://github.com/maifeeulasad/poppler-ts/issues"
  },
  "homepage": "https://github.com/maifeeulasad/poppler-ts#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "packageManager": "pnpm@10.15.0",
  "hasInstallScript": true,
  "dependencies": {
    "node-addon-api": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "node-gyp": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "gypfile": true
}
